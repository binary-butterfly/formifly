{
  "name": "formifly",
  "version": "2.1.2",
  "description": "React form handling as light as a butterfly",
  "main": "dist/umd/formifly.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/esm/*",
    "dist/umd/*",
    "dist/index.d.ts"
  ],
  "scripts": {
    "test": "jest --config jest.config.js ",
    "test:watch": "jest --config jest.config.js --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "coverage": "jest --config jest.config.js --collectCoverage",
    "watch": "rollup -c rollup.config.js -w",
    "build:dev": "rollup -c rollup.config.js",
    "build:prod": "NODE_ENV=production rollup -c rollup.config.js",
    "browse": "browser-sync start --s --ss build --index src/html/index.html --files dist/* --no-notify",
    "start": "npm-run-all --parallel watch browse",
    "prepublishOnly": "npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binary-butterfly/formifly.git"
  },
  "keywords": [
    "react",
    "forms"
  ],
  "author": "binary butterfly GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binary-butterfly/formifly/issues"
  },
  "homepage": "https://github.com/binary-butterfly/formifly#readme",
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.16.4",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.18.6",
    "@binary-butterfly/eslint-config": "^0.0.6",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.1",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^9.0.2",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.5.2",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "babel-plugin-styled-components": "^2.0.2",
    "browser-sync": "^2.27.7",
    "eslint": "^8.27.0",
    "i18next": "^22.0.6",
    "jest": "^27.4.7",
    "jsdom": "^19.0.0",
    "npm-run-all": "^4.1.5",
    "react-i18next": "^12.0.0",
    "rollup": "^2.58.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^4.2.3",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-terser": "^7.0.2",
    "styled-components": "^5.3.3",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
