{
  "name": "formifly",
  "version": "2.11.0",
  "description": "React form handling as light as a butterfly",
  "exports": {
    ".": {
      "types": "./dist/src/js/main.d.ts",
      "import": "./dist/formifly.js",
      "require": "./dist/formifly.umd.cjs"
    }
  },
  "main": "dist/formifly.umd.js",
  "module": "dist/formifly.js",
  "types": "dist/src/js/main.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "test": "vitest --run",
    "test:watch": "vitest --watch",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "coverage": "vitest --run --coverage",
    "build:dev": "vite build",
    "build:prod": "NODE_ENV=production vite build",
    "start": "vite",
    "prepublishOnly": "npm run build:prod"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/binary-butterfly/formifly.git"
  },
  "keywords": [
    "react",
    "forms"
  ],
  "author": "binary butterfly GmbH",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/binary-butterfly/formifly/issues"
  },
  "homepage": "https://github.com/binary-butterfly/formifly#readme",
  "devDependencies": {
    "@binary-butterfly/eslint-config": "^0.0.6",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^12.1.2",
    "@types/node": "^24.3.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^8.27.0",
    "i18next": "^22.0.6",
    "jest-styled-components": "^7.2.0",
    "jsdom": "^19.0.0",
    "react-i18next": "^12.0.0",
    "styled-components": "^5.3.3 || ^6.1.0",
    "tslib": "^2.5.0",
    "typescript": "^4.9.4",
    "vite": "^7.0.5",
    "vite-plugin-dts": "^3.7.1",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^17.0.2 || ^18.0.0",
    "react-dom": "^17.0.2 || ^18.0.0"
  }
}
